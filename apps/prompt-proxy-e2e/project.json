{
  "name": "prompt-proxy-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "implicitDependencies": ["prompt-proxy"],
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "jestConfig": "apps/prompt-proxy-e2e/jest.config.ts",
        "passWithNoTests": true
      },
      "dependsOn": ["prompt-proxy:build", "prompt-proxy:serve"]
    }
  }
}
