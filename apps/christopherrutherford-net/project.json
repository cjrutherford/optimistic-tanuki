{
  "name": "christopherrutherford-net",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/christopherrutherford-net/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/christopherrutherford-net",
        "index": "apps/christopherrutherford-net/src/index.html",
        "browser": "apps/christopherrutherford-net/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/christopherrutherford-net/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/christopherrutherford-net/public"
          }
        ],
        "styles": ["apps/christopherrutherford-net/src/styles.scss"],
        "scripts": [],
        "server": "apps/christopherrutherford-net/src/main.server.ts",
        "ssr": {
          "entry": "apps/christopherrutherford-net/src/server.ts"
        },
        "outputMode": "server"
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "4mb",
              "maximumError": "8mb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "continuous": true,
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "christopherrutherford-net:build:production"
        },
        "development": {
          "buildTarget": "christopherrutherford-net:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "christopherrutherford-net:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/christopherrutherford-net/jest.config.ts"
      }
    },
    "serve-static": {
      "continuous": true,
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "christopherrutherford-net:build",
        "port": 4200,
        "spa": true
      }
    },
    "server": {
      "dependsOn": ["build"],
      "executor": "@angular-devkit/build-angular:server",
      "options": {
        "outputPath": "dist/apps/christopherrutherford-net/server",
        "main": "apps/christopherrutherford-net/src/server.ts",
        "tsConfig": "apps/christopherrutherford-net/tsconfig.server.json",
        "inlineStyleLanguage": "scss"
      },
      "configurations": {
        "production": {
          "outputHashing": "media"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "sourceMap": true,
          "extractLicenses": false,
          "vendorChunk": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve-ssr": {
      "continuous": true,
      "executor": "@angular-devkit/build-angular:ssr-dev-server",
      "configurations": {
        "development": {
          "browserTarget": "christopherrutherford-net:build:development",
          "serverTarget": "christopherrutherford-net:server:development"
        },
        "production": {
          "browserTarget": "christopherrutherford-net:build:production",
          "serverTarget": "christopherrutherford-net:server:production"
        }
      },
      "defaultConfiguration": "development"
    },
    "prerender": {
      "executor": "@angular-devkit/build-angular:prerender",
      "options": {
        "routes": ["/"]
      },
      "configurations": {
        "development": {
          "browserTarget": "christopherrutherford-net:build:development",
          "serverTarget": "christopherrutherford-net:server:development"
        },
        "production": {
          "browserTarget": "christopherrutherford-net:build:production",
          "serverTarget": "christopherrutherford-net:server:production"
        }
      },
      "defaultConfiguration": "production"
    }
  }
}
