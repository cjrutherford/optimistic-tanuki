  "scripts": {
    "docker:build": "docker compose build",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "build:dev": "nx run-many --target=build --projects=authentication,client-interface,gateway,profile,social,project-planning,assets,forgeofwill,chat-collector,prompt-proxy,ai-orchestrator,telos-docs-service,blogging,christopherrutherford-net,digital-homestead --configuration=development",
    "build": "nx run-many --target=build --projects=authentication,client-interface,gateway,profile,social,project-planning,assets,forgeofwill,chat-collector,prompt-proxy,ai-orchestrator,telos-docs-service,blogging,christopherrutherford-net,digital-homestead",
    "watch:build": "nx watch --all -- nx run-many --target=build --projects=authentication,client-interface,gateway,profile,social,project-planning,assets,forgeofwill,chat-collector,prompt-proxy,ai-orchestrator,telos-docs-service,blogging,christopherrutherford-net,digital-homestead --configuration=development",
    "docker:build:dev": "docker compose -f docker-compose.yaml -f docker-compose.dev.yaml build",
    "docker:dev": "docker compose -f docker-compose.yaml -f docker-compose.dev.yaml up -d --force-recreate && alias dc-down='docker compose -f docker-compose.yaml -f docker-compose.dev.yaml down --remove-orphans && alias dc-down=\"\"'",
    "fow:docker:build:dev": "docker compose -f fow.docker-compose.yaml -f fow.docker-compose.dev.yaml build",
    "fow:docker:dev": "docker compose -f fow.docker-compose.yaml -f fow.docker-compose.dev.yaml up -d --force-recreate && alias fow-dc-down='docker compose -f fow.docker-compose.yaml -f fow.docker-compose.dev.yaml down --remove-orphans && alias fow-dc-down=\"\"'",
    "fow:docker:down": "docker compose -f fow.docker-compose.yaml -f fow.docker-compose.dev.yaml down --remove-orphans",
    "pre:docker:dev": "npm run build",
    "docker:dev:watch": "npm run build:dev && concurrently \"npm run docker:dev\" \"npm run watch:build\"",
    "db:setup": "sh ./setup-and-migrate.sh",
    "dev:ssr": "ng run christopherrutherford-net:serve-ssr",
    "serve:ssr": "node dist/apps/christopherrutherford-net/server/main.js",
    "build:ssr": "ng build && ng run christopherrutherford-net:server",
    "prerender": "ng run christopherrutherford-net:prerender"
  },
